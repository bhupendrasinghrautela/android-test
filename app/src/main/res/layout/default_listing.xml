                        app:itemProgressIcon="@drawable/star_selected"
                        app:itemSecondaryProgressIcon="@drawable/star_unselected"
                        app:itemWidth="@dimen/serp_listing_card_seller_rating_item_width" />
                    <!--style="?android:attr/ratingBarStyleSmall" />-->
