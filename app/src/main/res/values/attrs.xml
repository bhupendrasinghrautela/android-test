<?xml version="1.0" encoding="utf-8"?>

<!-- Custom styles to be declared here -->
<resources>

    <!--Custom style for expandablelayout -->
    <declare-styleable name="expandableLayout">
        <attr name="ael_duration" format="integer" />
        <attr name="ael_expanded" format="boolean" />
        <attr name="handle_resource" format="integer" />
        <attr name="header_text" format="string" />
        <attr name="footer_resource" format="integer" />
        <attr name="content_resource" format="integer" />
        <attr name="ael_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RatingBar">
        <attr name="itemMarginLeft" format="dimension" />
        <attr name="itemMarginRight" format="dimension" />
        <attr name="itemWidth" format="dimension" />
        <attr name="itemHeight" format="dimension" />
        <attr name="itemProgressIcon" format="reference" />
        <attr name="itemSecondaryProgressIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="MakaanProgressBar">
        <attr name="isLocality" format="boolean"/>
    </declare-styleable>

        <declare-styleable name="fontTextView">
            <attr name="fontName" format="string" />
        </declare-styleable>

</resources>